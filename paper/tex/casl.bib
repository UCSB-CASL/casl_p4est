@Article{Burman;Jacot;Picasso:04:Adaptive-finite-elem,
  Title                    = {Adaptive finite elements with high aspect ratio for the computation of coalescence using a phase-field model},
  Author                   = {E. Burman and A. Jacot and M. Picasso},
  Journal                  = {Journal of Computational Physics},
  Year                     = {2004},
  Number                   = {1},
  Pages                    = {153 - 174},
  Volume                   = {195},

  Abstract                 = {A multiphase-field model for the description of coalescence in a binary alloy is solved numerically using adaptive finite elements with high aspect ratio. The unknown of the multiphase-field model are the three phase fields (solid phase 1, solid phase 2, and liquid phase), a Lagrange multiplier and the concentration field. An Euler implicit scheme is used for time discretization, together with continuous, piecewise linear finite elements. At each time step, a linear system corresponding to the three phases plus the Lagrange multiplier has to be solved. Then, the linear system pertaining to concentration is solved. An adaptive finite element algorithm is proposed. In order to reduce the number of mesh vertices, the generated meshes contain elements with high aspect ratio. The refinement and coarsening criteria are based on an error indicator which has already been justified theoretically for simpler problems. Numerical results on two test cases show the efficiency of the method.},
  Bdsk-url-1               = {http://www.sciencedirect.com/science/article/pii/S0021999103005151},
  Bdsk-url-2               = {http://dx.doi.org/10.1016/j.jcp.2003.09.021},
  Date-added               = {2012-10-26 04:34:18 +0000},
  Date-modified            = {2014-12-05 04:51:47 +0000},
  Doi                      = {10.1016/j.jcp.2003.09.021},
