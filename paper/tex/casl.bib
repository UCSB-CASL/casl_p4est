	Doi = {10.1006/jcph.1998.5893},
	Issn = {0021-9991},
	Journal = {Journal of Computational Physics},
	Number = {2},
	Pages = {432 - 452},
	Title = {The Adjoint Method for an Inverse Design Problem in the Directional Solidification of Binary Alloys},
