	Issn = {0021-9991},
	Journal = {Journal of Computational Physics},
	Keywords = {Ammonium chloride},
	Number = {23},
	Pages = {9823 - 9840},
	Title = {Simulation of directional solidification, thermochemical convection, and chimney formation in a Hele-Shaw cell},
	Url = {http://www.sciencedirect.com/science/article/pii/S0021999108004099},
	Volume = {227},
	Year = {2008},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0021999108004099},
