  ISBN                     = {0042200903},
  ISSN                     = {1432-0770},
  Keywords                 = {Action Potentials,Action Potentials: physiology,Computer Simulation,Feedback,Mathematics,Models,Nerve Net,Nerve Net: physiology,Neurological,Neurons,Neurons: physiology,Periodicity},
  Pmid                     = {19911192},
