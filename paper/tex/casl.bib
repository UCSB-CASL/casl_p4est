  Bdsk-url-2               = {http://dx.doi.org/10.1016/j.jcp.2011.04.012},
  Date-added               = {2012-10-26 04:34:18 +0000},
  Date-modified            = {2014-12-05 04:51:48 +0000},
  Doi                      = {10.1016/j.jcp.2011.04.012},
  ISSN                     = {0021-9991},
  Keywords                 = {Isoperimetric problem},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0021999111002361}
}

