@Article{Bornia;Cervone;Manservisi;etal:11:On-the-properties-an,
  Title                    = {On the properties and limitations of the height function method in two-dimensional Cartesian geometry},
  Author                   = {Bornia, G. and Cervone, A. and Manservisi, S. and Scardovelli, R. and Zaleski, S.},
  Journal                  = {Journal of Computational Physics},
