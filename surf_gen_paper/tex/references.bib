	Issn = {0021-9991},
	Journal = {Journal of Computational Physics},
	Keywords = {Finite element methods},
	Number = {1},
	Pages = {214 - 238},
	Title = {Simulation of visible and ultra-violet group-III nitride light emitting diodes},
