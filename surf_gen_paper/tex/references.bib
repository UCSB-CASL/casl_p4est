	Issn = {0899-7667},
	Journal = {Neural Computation},
	Keywords = {Action Potentials,Action Potentials: physiology,Animals,Biological Clocks,Brain,Brain: physiology,Electric Stimulation,Humans,Models,Nerve Net,Nerve Net: physiology,Neurons,Neurons: physiology,Statistical,Synaptic Transmission,Synaptic Transmission: physiology},
