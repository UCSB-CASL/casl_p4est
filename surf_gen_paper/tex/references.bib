	Issn = {0021-9991},
	Journal = {Journal of Computational Physics},
	Number = {2},
	Pages = {384 - 411},
	Title = {Numerical Study of Convection in the Directional Solidification of a Binary Alloy Driven by the Combined Action of Buoyancy, Surface Tension, and Electromagnetic Forces},
